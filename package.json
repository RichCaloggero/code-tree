{
  "main": "codeTree.bundle.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-flow-strip-types",
      "transform-object-rest-spread",
      "transform-runtime"
    ]
  },
  "scripts": {
  "watch-wrap": "watchify codeTree-wrap.js -o codeTree.bundle.js --source-maps",
  "watch-trigger": "watchify codeTree-trigger.js -o codeTree.bundle.js --source-maps",
  "build-wrap": "browserify codeTree-wrap.js -o codeTree.bundle.js --source-maps",
  "build-trigger": "browserify codeTree-trigger.js -o codeTree.bundle.js --source-maps"
 
},
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "babylon": "^6.8.1",
    "source-map-support": "^0.4.0",
    "browserify": "^13.1.1",
    "escodegen": "^1.8.1",
    "esdispatch": "0.0.3",
    "esprima": "^3.1.3",
    "esquery": "^0.4.0",
    "estemplate": "^0.5.1"
  },
  "devDependencies": {
    "cst": "^0.4.9",
    "watchify": "^3.8.0"
  }
}
